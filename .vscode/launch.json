{
   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/SiA/bin/Debug/netcoreapp2.2/SiA.dll",
            "args": ["-f", "${workspaceFolder}/data/fm_gimmick_obj_alpha.xml.e"],
            "cwd": "${workspaceFolder}/src/SiA",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "SIA_DEBUG": "true",
                "SIA_WORKDIR": "${workspaceFolder}/data"
            }
        }
    ]
}